package eu.opendai.spain.apis;

public class Parada {
	 
	String name;
	String coord_x;
	String coord_y;
	
	Parada(){}
	
	Parada(String name, String coord_x, String coord_y){
		this.name=name;
		this.coord_x=coord_x;
		this.coord_y=coord_y;
	}
 
	public String getName() {
		return name;
	}
 
	public String getCoordX() {
		return coord_x;
	}
 
	public String getCoordY() {
		return coord_y;
	}
 
	public void setName(String name) {
		this.name = name;
	}
 
	public void setCoordX(String in_x) {
		this.coord_x= in_x;
	}
 
	public void setCoordY(String in_y) {
		this.coord_y= in_y;
	}
}